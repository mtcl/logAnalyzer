<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.2.xsd">

	<!-- Define the single wily alert date time here -->
	<bean id="startDate" class="java.util.Date">
		<constructor-arg value="01/23/13 19:00:00" />
	</bean>

	<!-- define for how long analysis is needed here -->
	<bean id="delta" class="java.lang.Integer">
		<constructor-arg value="60" />
	</bean>

</beans>
